{
  "name": "analgo/tools",
  "version": "1.0.1",
  "description": "A PHP library to write and track visitor",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Mansour Marouan",
      "email": "marouan.ben.mansour@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Analgo\\Tools\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "psr/log": "^1.1 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.11"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR12 src/"
  }
}
